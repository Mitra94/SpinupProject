<p id="notice"><%= notice %></p>

<h1>Listing Developers</h1>

<% if @developers.present? %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Surname</th>
      <th>Age</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @developers.each do |developer| %>
      <tr>
        <td><%= developer.name %></td>
        <td><%= developer.surname %></td>
        <td><%= developer.age %></td>
        <td><%= developer.city %></td>
        <td><%= link_to 'Show', developer %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% else %>
  <p>There are no developers containing the term(s) <%= params[:search] %>.</p>
<% end %>

<br>

